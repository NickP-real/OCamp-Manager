{
	"name": "ocampmanager",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"db:up": "drizzle-kit up",
		"db:migrate": "drizzle-kit migrate",
		"db:check": "drizzle-kit check",
		"db:studio": "drizzle-kit studio",
    "db:seed": "bun run ./src/db/seeds/index.ts",
    "db:nuke": "bun run ./src/db/scripts/nuke.ts",
		"db-vercel:push": "drizzle-kit push --config ./drizzle.config.vercel.ts",
		"db-vercel:migrate": "drizzle-kit migrate --config ./drizzle.config.vercel.ts",
		"db-vercel:check": "drizzle-kit check --config ./drizzle.config.vercel.ts",
		"db-vercel:studio": "drizzle-kit studio --config ./drizzle.config.vercel.ts"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@fontsource/fira-mono": "^5.0.14",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.46.1",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/kit": "^2.5.25",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/forms": "^0.5.8",
		"@types/eslint": "^8.56.12",
		"@types/pg": "^8.11.7",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.21.4",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"sveltekit-superforms": "^2.17.0",
		"tailwindcss": "^3.4.10",
		"tslib": "^2.7.0",
		"typescript": "^5.5.4",
		"vite": "^5.4.2",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.2",
		"@tanstack/svelte-table": "^8.20.5",
		"@vercel/postgres": "^0.8.0",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.13",
		"dotenv-flow": "^4.1.0",
		"drizzle-orm": "^0.30.10",
		"drizzle-zod": "^0.5.1",
		"lucide-svelte": "^0.378.0",
		"nanoid": "^5.0.7",
		"pg": "^8.12.0",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1",
		"zod": "^3.23.8"
	},
	"packageManager": "bun@1.1.26"
}
